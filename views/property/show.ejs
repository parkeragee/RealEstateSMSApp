<div class="container">
	<div class="app-main show">
		<div class="row">
			<div class="col-md-6">
				<h1><%= property.streetAddress %><br>
				<%= property.city %>, <%= property.state %> <%= property.zip %></h1>
			</div>
		</div>
		<div class="no-description">No property description. <a href="/property/edit/<%= property.id %>">Add one</a></div>
		<div class="description"><p><%= property.description %></p></div>
		<ul>
			<li># of Bedrooms: <%=property.bedrooms %></li>
			<li># of Bathrooms: <%=property.bathrooms %></li>
			<li>Square footage: <%=property.squareFeet %></li>
			<li>Asking price: <%=property.askingPrice %></li>
		</ul>
	</div>
</div>